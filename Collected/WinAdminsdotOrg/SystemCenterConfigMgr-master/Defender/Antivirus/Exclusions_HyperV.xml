<SecurityPolicy Name="Exclusions: Hyper-V" Description="" CreatedBy="CORP\LabAdmin" LastModifiedBy="CORP\LabAdmin" xmlns="http://forefront.microsoft.com/FEP/2010/01/PolicyData">
  <PolicySection Name="FEP.AmPolicy" Disabled="false">
    <LocalGroupPolicySettings>
